<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_color">

    <TextView
        android:id="@+id/sawiq5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ques5"
        android:textSize="20sp"
        android:textColor="@color/secondary_color"
        android:textStyle="bold"
        android:layout_marginTop="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/cb_SW5_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/k01"
        app:layout_constraintTop_toBottomOf="@id/sawiq5"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_SW5_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/k03"
        app:layout_constraintTop_toBottomOf="@id/cb_SW5_1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_SW5_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/k04"
        app:layout_constraintTop_toBottomOf="@id/cb_SW5_2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_SW5_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/k05"
        app:layout_constraintTop_toBottomOf="@id/cb_SW5_3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_SW5_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/k06"
        app:layout_constraintTop_toBottomOf="@id/cb_SW5_4"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_SW5_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/k07"
        app:layout_constraintTop_toBottomOf="@id/cb_SW5_5"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_SW5_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/k08"
        app:layout_constraintTop_toBottomOf="@id/cb_SW5_6"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_SW5_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/k09"
        app:layout_constraintTop_toBottomOf="@id/cb_SW5_7"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_SW5_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/k12"
        app:layout_constraintTop_toBottomOf="@id/cb_SW5_8"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp" />

    <Button
        android:id="@+id/buttonNextSW5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Selanjutnya"
        app:layout_constraintTop_toBottomOf="@id/cb_SW5_9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
